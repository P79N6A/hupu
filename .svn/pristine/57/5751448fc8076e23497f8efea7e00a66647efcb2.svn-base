	<script>
		function checkURL(URL) {
			var Cts = URL;
			if (Cts.indexOf("#") >= 0) {
				return false;
			}
			return true;
		}

		function showmenurl() {
			console.log(2);
			$("#menu").css({
				"transform": "translate3d(0px, 0px, 0px)",
				"transition-duration": "400ms",
				"visibility": "visible"
			});
			$(".bg").show();
		}
		$(function () {
			$('a,.a-b').on('click', function () {
				console.log(3)
				var href = $(this).attr('href');
				if (checkURL(href)) {
					top.location.href = href;
				}

			});

			$('.bg').on('click', function () {
				console.log(1)
				$("#menu").css({
					"transform": "",
					"transition-duration": "0ms",
					"visibility": "hiiden"
				});
				$(".bg").hide();
			});

		})
	</script>
