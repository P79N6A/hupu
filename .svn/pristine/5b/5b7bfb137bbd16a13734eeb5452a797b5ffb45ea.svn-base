<script>
    //			console.log('{$share_id}');
    var unionid = "{$unionid}";
    var sortlist = [{
        "template_name": '产品中心',
        "template_classname": "cenc"
    },
        {
            "template_name": '展示一切',
            "template_classname": "cene"
        },
        {
            "template_name": '视频',
            "template_classname": "cend"
        },
        {
            "template_name": '链接一切',
            "template_classname": "cenb"
        },
        {
            "template_name": '个人展示',
            "template_classname": "cena"
        }
    ];
    //
    $.ajax({
        type: 'post',
        url: '/index.php?s=/Api/NewCardApi/plate_order_list',
        data: {
            unionid: unionid
        },
        async: false,
        success: function(res) {
            //                    console.log(res)
            if(res.Message == "操作成功") {
                var _sort_list = [];
                _sort_list[parseInt(res.AppendData["产品中心"])] = {
                    "template_name": '产品中心',
                    "template_classname": "cenc"
                };
                _sort_list[parseInt(res.AppendData["展示一切"])] = {
                    "template_name": '展示一切',
                    "template_classname": "cene"
                };
                _sort_list[parseInt(res.AppendData["视频"])] = {
                    "template_name": '视频',
                    "template_classname": "cend"
                };
                _sort_list[parseInt(res.AppendData["链接一切"])] = {
                    "template_name": '链接一切',
                    "template_classname": "cenb"
                };
                _sort_list[parseInt(res.AppendData["个人展示"])] = {
                    "template_name": '个人展示',
                    "template_classname": "cena"
                };
                var _list = [];
                for(var index = 0; index < _sort_list.length; index++) {
                    if(_sort_list[index] == null || _sort_list[index] == undefined) {
                        continue;
                        //console.log(index);
                    }
                    _list.push(_sort_list[index]);

                }
                //						console.log(_sort_list);
                sortlist = _list;

            }
            init_sort();
        },
        error: function() {
            console.log("顺序")
        }
    })

    function init_sort() {
        // console.log(sortlist);
        var obj = {
            "sortlist": sortlist
        };
        $("#sort_content").html(template("sort_content_sort_sort", obj));
    }
    move = true;
    $(".top_cen").on('click', function() {
        $(".masks").show()
        $(".side").css({
            "transform": 'translateX(100%)',
            "transition": 'transform .2s'
        });
    });
    $(".masks").on('click', function() {
        $(".masks").hide();
        $(".hint").hide();
        $(".hinte").hide();
        $(".side").css({
            "transform": 'translateX(0%)',
            "transition": 'transform .2s'
        });
    });

    //中间滑动
    //滑动处理
    var startX, moveEndX, X, Y, X1, Y1, n = 10000,
        _this, time = 0,
        id;
    $(".banner>div").on('touchstart', function(e) {
        //e.preventDefault();
        startX = e.touches[0].pageX;

        startY = e.touches[0].pageY;
    });
    $(".banner>div").on('touchend', function(e) {
        if(time == 1) {
            return;
        }
        time = 1;
        setTimeout(function() {
            time = 0
        }, 500)

        moveEndX = e.changedTouches[0].pageX;
        moveEndY = e.changedTouches[0].pageY;
        X = moveEndX - startX;
        Y = moveEndY - startY;
        X1 = Math.abs(X)
        Y1 = Math.abs(Y)
        if(Y1 > X1) {
            return
        }
        if(X > 50) {
            $('.con_cens').animate({
                scrollTop: '1px'
            })
            n++;
            var name = $(this).attr("name");
            if(name == 0) {
                name = 4
            } else {
                name--
            }
            var index = $(".banner>div").eq(name).css("z-index");
            // ID值
            id = name;

            $(".banner>div").eq(name).css({
                "transform": "translate(0%,0%) rotateZ(0deg)",
                "transition": "all 0.5s",
                "z-index": index
            });
            if(name == 0) {
                name = 4
            } else {
                name--;
            }
            // 备用项
            var index = $(".banner>div").eq(name).css("z-index");
            index = Number(index);
            index = index + 5;
            $(".banner>div").eq(name).css({
                "transform": "translate(-200%,200%) rotateZ(-30deg)",
                "transition": "all 0s",
                "z-index": index
            });
        } else if(X < -50) {
            $('.con_cens').animate({
                scrollTop: '1px'
            })
            var name = $(this).attr("name");
            // 当前滑动项
            $(this).css({
                "transform": "translate(-200%,200%) rotateZ(-30deg)",
                "transition": "all 0.8s"
            });
            var present = name;
            if(name == 0) {
                name = 4
            } else {
                name--
            }
            // 上一个滑动项
            var index = $(".banner>div").eq(name).css("z-index") - 5;
            $(".banner>div").eq(name).css({
                "transform": "translate(0%,0%) rotateZ(0deg)",
                "transition": "all 0s",
                "z-index": index
            });
            present == 4 ? present = 0 : present++;
            id = present;
        }
        myfun();
    });

