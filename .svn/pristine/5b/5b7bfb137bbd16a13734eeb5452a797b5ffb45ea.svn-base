    // 检查一开始进入页面
    var clock;
    $(".show,.t_link,.v_pro,.con_cens").on("scroll", function() {
        if(clock) { // 如果在300毫秒内进行scroll的话，都会被clearTimeout掉，setTimeout不会执行。
            //如果有300毫秒外的操作，会得到一个新的timeID即clock，会执行一次setTimeout,然后保存这次setTimeout的ID，
            //对于300毫秒内的scroll事件，不会生成新的timeID值，所以会一直被clearTimeout掉，不会执行setTimeout.
            clearTimeout(clock);
        }
        clock = setTimeout(function() {
            //					console.log('运行了一次');
            lazyRender();
        }, 300)
    });

    function checkShow($img) { // 传入一个img的jq对象
        var scrollTop = $('.vimg').scrollTop(); //即页面向上滚动的距离
        var windowHeight = $('.banner').height(); // 浏览器自身的高度
        var offsetTop = $img.offset().top; //目标标签img相对于document顶部的位置
        if(offsetTop < (scrollTop + windowHeight) && offsetTop > scrollTop) { //在2个临界状态之间的就为出现在视野中的
            return true;
        } else {
            return false;
        }
    };

    function isLoaded($img) {
        return $img.attr('data-src') === $img.attr('src'); //如果data-src和src相等那么就是已经加载过了
        //				console.log('判断是否加载了')
    };

    function loadImg($img) {
        $img.attr('src', $img.attr('data-src')); //加载就是把自定义属性中存放的真实的src地址赋给src属性
        //				console.log('走了加载图片的方法')
    };

    function lazyRender() {
        $('.lazyimg').each(function() {
            if(checkShow($(this)) && !isLoaded($(this))) {
                loadImg($(this));
                //						console.log("第一次加载")
            }
        })
    };
</script>


