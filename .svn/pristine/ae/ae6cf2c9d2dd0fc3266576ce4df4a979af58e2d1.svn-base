<!-- 年龄动态数据 -->
<script type="text/html" id="age">
	{{each AppendData v i}}
	<li data-id="{{v.id}}" class="fl">{{v.age_group}}</li>
	{{/each}}
</script>

<!-- 行业数据 -->
<script type="text/html" id="industry">
	{{each AppendData v i}}
	<div class="industry_msg">
		
		<p data-id="{{v.id}}"><span class="xian"></span>{{v.name}}</p>
		<ul class="clearfix industryUl">
			{{each v.two_list v i}}
			<li id="All_li" class="fl" data-id="{{v.id}}">{{v.name}}</li>
			{{/each}}
		</ul>
	</div>
	{{/each}}
</script>
<!-- 用户信息 -->
<script type="text/html" id="user_msg">
	{{each userlist v i}}
	<li class="user_msg">
		<a href="__ROOT__/index.php?s=/Home/Nologin/card_jump/id/{{v.id}}">
			<div class="img_box fl">
				<img class="nick_msg fl" src="{{v.headimg}}" alt="">
			</div>
			<div class="u_infor">
				<div class="user_msg_box" detail='{{v.unionid}}'>
					<div class="nick_name">{{v.nick_name}}</div>
				</div>
				
				<div class="clearfix user_bottom">
					{{if v.address}}
					<div class=" text">
						{{v.address}}
					</div>
					{{else if v.address == null}}
					<div class=" text">
						用户未填写
					</div>
					{{else}}
					<div class=" text">
					用户未填写
					</div>
					{{/if}}
				</div>
				
				<div class="here">
					<img class="fl imgs" src="__HOME_IMAGES__/newvnet/business/shangquan_nearby_icon@2x.png" alt="">
					{{if v.longitude == "" && v.latitude == ""}}
						<div class="len fl">
							未获取
						</div>
						{{else if v.distance == 0}}
						<div class="len fl">
							0m
						</div>
						{{else if v.distance <= 0.1}}
						<div class="len fl">
							100m内
						</div>
						{{ else if v.distance <= 0.2}}
						<div class="len fl">
							200m内
						</div>
						{{ else if v.distance <= 0.3}}
						<div class="len fl">
							300m内
						</div>
						{{ else if v.distance <= 0.4}}
						<div class="len fl">
							400m内
						</div>
						{{ else if v.distance <= 0.5}}
						<div class="len fl">
							500m内
						</div>
						{{ else if v.distance <= 1}}
						<div class="len fl">
							1km内
						</div>
						{{ else if v.distance > 1}}
						<div class="len fl">
							{{v.distance}}km
						</div>
					{{/if}}
				</div>
			</div>	
		</a>
		{{if v.is_friend == 0}}
			<a href="javascript:;" class="gg add_fri">
				+Ta聊天
			</a>
		{{/if}}
		
		{{if v.is_friend == 2}}
			<a href="javascript:;" class="gg yj">
				已添加
			</a>
		{{/if}}
		
		{{if v.is_friend == 1 }}
			<div class="lnk">
				<a href="http://mingxin365.com:2001/webdemo/h5/login.html?uname={$phone}&friend={{v.phone}}">
					聊天
				</a>
			</div>
		{{/if}}
	
		<!--{{if v.is_friend == 0}}
		<a class="gg add_fri" href="javascript:;">
			+Ta聊天
		</a>
		{{else}}
		<a href="http://mingxin365.com:2001/webdemo/h5/login.html?uname={$phone}&friend={{v.phone}}" class="gg yj">
          	聊天
        </a> 
		{{/if}}-->

	</li>
	{{/each}}
</script>
