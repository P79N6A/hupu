<script>
	$(function(){


	var openid = '{$openid}';
	console.log(openid)
    var returndata=[];
	 $.ajax({
		url: "/index.php?s=/Api/Vote/init_prize_page",
		type: "post",
		data: {
			openid: openid
		},
		success: function (res) {
			console.log(res)
			var list = res.AppendData.initdata;

			var firstPart = [];
			var secondPart = [];
			var thirdPart = [];
			var params = {}
			for (var i = 0; i < list.length; i++) {
				list[i]['index'] = i;
				if (i <= 2) {
					firstPart.push(list[i])
				} else if (i > 2 && i <= 4) {
					secondPart.push(list[i])
				} else {
					thirdPart.push(list[i])
				}

			}

			params['firstPart'] = firstPart;
			params['secondPart'] = secondPart;
			params['thirdPart'] = thirdPart;

			console.log(params)
			var temp = template('prize', params)
			$('#luck').html(temp);
			var btn = $("#btn")
			$("#btn").prev().before(btn);
			luck.init('luck');


			$("#luck").on("click", "#btn", function () {
                if (click) {
                    return false;
                }else{

				$.ajax({
					url:"/index.php?s=/Api/Vote/luck_draw",//'{:url("/Api/Vote/luck_draw")}',
					type: "POST",
					data: {
						openid: openid
					},
					success: function (res) {
						console.log(res)
						if(res.ResultType=="0"){
						    if(res.Message=="用户不存在"){
						        alert("用户不存在");
                                click = true;
						        return false;
							}else if(res.Message=="您的抽奖次数已用完"){
                                $(".go_draw").show();
								$(".mask").show();
                                click = true;
                                return false;
							}else if(res.Message=="抽奖人数太多了，下次在来吧"){
                                alert("抽奖人数太多了，下次在来吧");
                                click = true;
                                return false;
							}
                            returndata=res.AppendData;
                            prize_index = res.AppendData.key
                            prid = parseInt(prize_index)
                            // } else {
                                luck.speed = 100;
                                draw(prid);
                                click = true;
                                return false;
                            // }
						}else{
                            click = true;
						    // alert("服务器繁忙");
						}





					}
				})


                }
			});

		}


	})

	var click = false;
	var prize_index;
	var prid;
	var luck = {
		index: 0, //当前转动到哪个位置，起点位置
		count: 0, //总共有多少个位置
		timer: 0, //setTimeout的ID，用clearTimeout清除
		speed: 20, //初始转动速度
		times: 1, //转动次数
		cycle: 50, //转动基本次数：即至少需要转动多少次再进入抽奖环节
		prize: -1, //中奖位置
		init: function (id) {
			if ($("#" + id).find(".luck-unit").length > 0) {
				$luck = $("#" + id);
				$units = $luck.find(".luck-unit");
				this.obj = $luck;
				this.count = $units.length;
				$luck.find(".luck-unit-" + this.index).addClass("active");
			};
		},


		roll: function () {
			var index = this.index;
			var count = this.count;
			var luck = this.obj;
			$(luck).find(".luck-unit-" + index).removeClass("active");
			index += 1;
			if (index > count - 1) {
				index = 0;
			};
			$(luck).find(".luck-unit-" + index).addClass("active");
			this.index = index;
			return false;
		},
		stop: function (index) {
			this.prize = index;
			return false;
		}
	};

