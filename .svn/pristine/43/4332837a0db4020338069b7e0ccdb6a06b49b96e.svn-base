	function draw() {
		luck.times += 1;
		// console.log("luck.times==>" + luck.times + "luck.cycle===>" + luck.cycle + " luck.prize==>" + luck.prize +
		// 	"luck.index==>" + luck.index);
		luck.roll();
		if (luck.times > (luck.cycle + 10) && luck.prize == luck.index) {
			clearTimeout(luck.timer);
			luck.prize = -1;
			luck.times = 0;
			console.log("抽奖结束")
			setTimeout(function(){
				showS();
			},600)
			
			click = false;
		} else {
			if (luck.times < luck.cycle) {
				luck.speed -= 10;
			} else if (luck.times == luck.cycle) {
				// var index = Math.random()*(luck.count)|0;
				// if(index>5){
				//     index = 7;
				// }else {
				//     index = 5;
				// }
				// luck.stop(prid)
				luck.prize = prid; //最终中奖位置
				// console.log("luck.cycle===>" + luck.prize)

			} else {
				if (luck.times > luck.cycle + 10 && ((luck.prize == 0 && luck.index == 7) || luck.prize == luck.index + 1)) {
					luck.speed += 110;
				} else {
					luck.speed += 20;
				}
			}
			if (luck.speed < 40) {
				// console.log("luck.cycleasdad===>" + luck.speed)
				luck.speed += 40;
			};

			luck.timer = setTimeout(draw, luck.speed);
		}
		return false;
	}


