			//单个删除
			$(document).on("click", ".d_img", function(event) {
				var d_id = $(this).attr("data-id")
				var unionid = "{$unionid}";
				event.stopPropagation();
                event.preventDefault();
				$.ajax({
					url: "/index.php?s=/Api/NewShowListApi/show_list_del",
					type: 'post',
					data: {
						unionid: unionid,
						show_ids: d_id
					},
					success: function(res) {
						console.log(res);
						window.location.reload();
					},
				})
			})

			//排序保存
			var content = ""
			$(".save").on("click", function() {
				var unionid = "{$unionid}";
				var arr = [];
				var arry = [];
				var str = ""
				$("#dr_cen .a_img").each(function(i, e) {
					arr.push($(this).attr("data-id"));
					arry.push(i + 1);
					content = "" + arr[i] + "=" + arry[i] + ""
					str += '' + content + '' + ','
				});
				console.log(str);
				$.ajax({
					url: "/index.php?s=/Api/NewShowListApi/show_list_sort",
					type: 'post',
					data: {
						unionid: unionid,
						content: str,
					},
					success: function(res) {
						console.log(res)
						window.location.reload();
					},
				})

			})
			$(".del_right").on("click", function() {
				var unionid = "{$unionid}";
				var id_array = new Array();
				$('input[name="id"]:checked').each(function() {
					id_array.push($(this).val()); //向数组中添加元素  
				});
				var idstr = id_array.join(','); //将数组元素连接起来以构建一个字符串  

				$.ajax({
					url: "/index.php?s=/Api/NewShowListApi/show_list_del",
					type: 'post',
					data: {
						unionid: unionid,
						show_ids: idstr
					},
					success: function(res) {
						//						console.log(res);
						window.location.reload();
					},
				})
			})
			//图文数量
			var unionid = "{$unionid}";
			var type_id = "{$type_id}";
			$.ajax({
				type: 'post',
				url: '/index.php?s=/Api/AppApi/show_list',
				dataType: 'json',
				data: {
					unionid: unionid,
					type_id: type_id
				},
				success: function(res) {

					$(".t_num").html(res.AppendData.length)

				},
			});

			$('.saves').on('click', function() {

				var id_array = new Array();
				$('input[name="id"]:checked').each(function() {
					id_array.push($(this).val()); //向数组中添加元素  
				});
				console.log(id_array.length);

				if(id_array.length == 0) {
					$(".model").show();
					$(".model").html('请至少选择一个移动项');
					setTimeout(function() {
						$(".model").hide();
					}, 1500)
					return false;
				}
				var idstr = id_array.join(','); //将数组元素连接起来以构建一个字符串  
				console.log(idstr)

				location.href = '/index.php?s=/Home/vhome/move&str=' + idstr + ''

			})
