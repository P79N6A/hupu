		<script>
			$(".top").html('{$title}')
			var isok = true;
			//选项切换
			$(".li").click(function() {
				$(".add_img").eq($(".li").index(this)).addClass("on").siblings().removeClass('on');
			});

			//移动
			$(".move").click(function() {
				$(".d_img").hide();
				$(".xz").show();
				$(".xzs").show();
				$(".confirm").hide();
				$(".delete").hide();
				$(".confirms").show();

			});

			//排序
			$(".sort").click(function() {
				$(".d_img").show();
				$(".confirm").show();
				$(".delete").hide();
				isok = false;
				//				$('.out').animate({
				//					'height':"0px"
				//				})
			})
			//删除
			$(".del").click(function() {
				$(".d_img").hide();
				$(".xz").show();
				$(".xzs").show();
				$(".confirm").hide();
				$(".delete").show();
				//				$('.out').animate({
				//					'height':"0px"
				//				})

			});

			$("#qx").click(function() {
				var qxs = $(this).prop("checked");
				var ck = $(".xz").prop("checked", qxs);
			})
			var unionid = "{$unionid}";
			var type_id = "{$type_id}";
			var title = "{$title}"
			console.log(type_id)
			var length = 0;
			var list = "";
			var lists = "";
			$.ajax({
				type: 'post',
				url: '/index.php?s=/Api/AppApi/show_list',
				dataType: 'json',
				data: {
					unionid: unionid,
					type_id: type_id,
					start: 0,
					length: 1000
				},
				success: function(res) {
					if(res.AppendData == "") {
						$(".add_cen").html("您还没有图文哦!请去添加图文")
						$(".d_cen").html("您还没有图文哦!请去添加图文");
					}
					if(res.ResultType == 0) {
						$(res.AppendData).each(function(v, i) {
							list += '<div class="a_img">'
							list += '<a href="/index.php?s=/Home/vhome/addgraphic/type_id/' + type_id + '/show_id/' + i.id + '/title/'+title+'">'
							list += '<div class="i_img">'
							list += '<img src="' + i.thumb + '" alt="" />'
							list += '</div>'
							list += '<div class="a_tit">' + i.title + '</div>'
							list += '<div class="d_img" data-id="' + i.id + '">'
							list += '<img src="__HOME_IMAGES__/newvnet/imgtxt/icon_jin_delete@2x.png" alt="" />'
							list += '</div>'
							list += '<label class="checkBox"><input name="id" class="xz" value="' + i.id + '" type="checkbox"></label>'
							list += '</a>'
							list += '</div>'
							lists += '<div class="a_img" data-id="' + i.id + '">'
							lists += '<a href="/index.php?s=/Home/vhome/addgraphic/type_id/' + type_id + '/show_id/' + i.id + '">'
							lists += '<div class="i_img">'
							lists += '<img src="' + i.thumb + '" alt="" />'
							lists += '</div>'
							lists += '<div class="a_tit">' + i.title + '</div>'
							lists += '<div class="d_img">'
							lists += '<img src="__HOME_IMAGES__/newvnet/imgtxt/icon_jin_paixu@2x.png" alt="" />'
							lists += '</div>'
							lists += '<label class="checkBox"><input class="xz" type="checkbox"></label>'
							lists += '</a>'
							lists += '</div>'
						})
						$(".add_cen").html(list);
						$(".d_cen").html(lists);
					}
				},
			});
			

