<!DOCTYPE html>
<html>

	<head>
		<meta charset="UTF-8" />
		<meta name="viewport" content="width=device-width,minimum-scale=1,maximum-scale=1,initial-scale=1,user-scalable=no" />
		<meta http-equiv="X-UA-Compatible" content="ie=edge" />
		<title>{$title}</title>
		<script src="__HOME_JS__/yb/js/rem.js"></script>
		<script src="__HOME_JS__/yb/js/jquery-3.1.1.min.js"></script>
		<style>
			* {
				margin: 0;
				padding: 0;
			}
			
			body {
				background: #fff;
			}
			
			.out {
				width: 100%;
				height: 6.35rem;
				background: #F6F6F6;
				padding-bottom: 0.8rem;
				position: fixed;
				top: 0;
				left: 0;
				z-index: 100000;
			}
			
			.top {
				width: 90%;
				height: 2.3rem;
				line-height: 3rem;
				font-size: 0.8rem;
				font-weight: 600;
				color: #333;
				margin: 0 auto;
			}
			
			.tab {
				width: 90%;
				height: 2.05rem;
				line-height: 1.5rem;
				color: #999;
				font-size: 0.6rem;
				margin: 0 auto;
			}
			
			.tab_left {
				float: left;
				width: 35%;
				overflow: hidden;
				text-overflow: ellipsis;
				white-space: nowrap;
			}
			
			.move {
				width: 20%;
				height: 100%;
				float: left;
				text-align: right;
				overflow: hidden;
			}
			
			.move_img {
				display: block;
				float: left;
				width: 1rem;
				height: 100%;
			}
			
			.move_img>img {
				display: block;
				width: 0.8rem;
				height: 0.75rem;
				margin-top: 0.35rem;
			}
			
			.del {
				width: 25.5%;
				height: 100%;
				float: left;
				text-align: right;
				overflow: hidden;
			}
			
			.del_img {
				display: block;
				float: left;
				width: 1rem;
				height: 100%;
			}
			
			.del_img>img {
				display: block;
				width: 0.8rem;
				height: 0.75rem;
				margin-top: 0.35rem;
			}
			
			.del_txt,
			.sort_txt,
			.move_txt {
				width: auto;
				height: 100%;
				float: left;
			}
			
			.sort {
				width: 19.5%;
				height: 100%;
				float: left;
				text-align: right;
				overflow: hidden;
			}
			
			.sort_img {
				display: block;
				float: left;
				width: 0.8rem;
				height: 100%;
			}
			
			.sort_img>img {
				display: block;
				width: 0.55rem;
				height: 0.75rem;
				margin-top: 0.35rem;
			}
			
			.add {
				width: 90%;
				height: 2rem;
				background: #F8C85E;
				border-radius: 0.2rem;
				margin: 0 auto;
				text-align: center;
				line-height: 2rem;
				font-size: 0.8rem;
				font-weight: 600;
				color: #fff;
			}
			
			.add>a {
				display: block;
				text-decoration: none;
				color: #fff;
			}
			
			.max {
				width: 100%;
				height: auto;
				/*overflow: hidden;*/
				position: relative;
				margin-top: 6.35rem;
			}
			
			.add_img {
				width: 90%;
				height: auto;
				padding: 1.5rem 0;
				background: #fff;
				margin: 0 auto 2rem;
				overflow: auto;
				position: absolute;
				top: 0;
				left: 5%;
				display: none;
			}
			
			.add_cen,
			.d_cen {
				width: 100%;
				height: auto;
				overflow: hidden;
				font-stretch: 0.8rem;
				color:#333;
				text-align: center;
				line-height:2.5rem ;
			}
			
			.a_img {
				width: 31.114%;
				height: 7.2rem;
				margin-bottom: 0.5rem;
				float: left;
				position: relative;
			}
			
			.a_img>a {
				text-decoration: none;
				display: block;
				width: 100%;
				height: 100%;
			}
			
			.a_img:nth-child(3n-1) {
				margin: 0 3.329%;
			}
			
			.i_img {
				width: 100%;
				height: 6rem;
			}
			
			.i_img>img {
				width: 100%;
				height: 100%;
				border-radius: 0.1rem;
			}
			
			.a_tit {
				width: 100%;
				height: 1.2rem;
				line-height: 1.2rem;
				font-size: 0.6rem;
				color: #777777;
				overflow: hidden;
				text-overflow: ellipsis;
				white-space: nowrap;
			}
			
			.on {
				display: block;
			}
			
			.add>span {
				margin-right: 0.25rem;
			}
			
			.add>a>span>img {
				width: 0.9rem;
				height: 0.8rem;
				vertical-align: middle;
			}
			
			.d_img {
				width: 0.9rem;
				height: 0.9rem;
				position: absolute;
				top: 0.4rem;
				right: 0.4rem;
				z-index: 1000;
				cursor: pointer;
			}
			
			.d_img>img {
				display: block;
				width: 100%;
				height: 100%;
			}
			
			.confirm {
				width: 100%;
				height: 2rem;
				background: #fff;
				padding: 0.2rem 0;
				position: fixed;
				bottom: 0;
				left: 0;
				z-index: 1000;
				display: none;
			}
			
			.save {
				width: 90%;
				height: 2rem;
				margin: 0 auto;
				background: #32CEA0;
				font-size: 0.8rem;
				font-weight: 600;
				text-align: center;
				color: #fff;
				letter-spacing: 0.25rem;
				line-height: 2rem;
				border-radius: 0.2rem;
			}
			
			.confirms {
				width: 100%;
				height: 2rem;
				background: #fff;
				padding: 0.2rem 0;
				position: fixed;
				bottom: 0;
				left: 0;
				z-index: 1000;
				display: none;
			}
			
			.saves {
				width: 90%;
				height: 2rem;
				margin: 0 auto;
				background: #32CEA0;
				font-size: 0.8rem;
				font-weight: 600;
				text-align: center;
				color: #fff;
				letter-spacing: 0.25rem;
				line-height: 2rem;
				border-radius: 0.2rem;
			}
			/*删除按钮*/
			
			.xz {
				width: 0.9rem;
				height: 0.9rem;
				-webkit-appearance: none;
				border: 0.05rem solid rgba(0, 0, 0, 0.5);
				border-radius: 50%;
				outline: none;
				position: absolute;
				top: 0.4rem;
				right: 0.4rem;
				display: none;
			}
			
			.xzs {
				display: block;
				width: 0.9rem;
				height: 0.9rem;
				-webkit-appearance: none;
				border: 2px solid rgba(0, 0, 0, 0.5);
				border-radius: 50%;
				outline: none;
				margin: 0.95rem 0.5rem 0.95rem 0;
				/*display: none;*/
			}
			
			.xz:checked {
				background: url("__HOME_IMAGES__/newvnet/imgtxt/icon_lian_lv@2x.png")no-repeat center;
				background-size: 100% 100%;
				border: 0.05rem solid #32CEA0;
				border-radius: 50%;
			}
			
			.xzs:checked {
				background: url("__HOME_IMAGES__/newvnet/imgtxt/icon_lian_lv@2x.png")no-repeat center;
				background-size: 100% 100%;
				border: 0.05rem solid #32CEA0;
				border-radius: 50%;
			}
			
			.delete {
				width: 90%;
				height: 2.8rem;
				background: #fff;
				position: fixed;
				left: 0;
				bottom: 0;
				border-top: 0.05rem solid #F6F6F6;
				padding: 0 5%;
				display: none;
				overflow: hidden;
				z-index: 1000000;
			}
			
			.del_left {
				width: 50%;
				height: 100%;
				display: flex;
				color: #777777;
				font-size: 0.6rem;
				line-height: 2.8rem;
				float: left;
			}
			
			.del_right {
				width: 50%;
				height: 2rem;
				background: #32CEA0;
				border-radius: 0.2rem;
				margin: 0.4rem 0;
				line-height: 2rem;
				text-align: center;
				font-size: 0.8rem;
				color: #fff;
				letter-spacing: 0.25rem;
				font-weight: 600;
				float: left;
			}
			/*删除按钮*/
			/*加载动效*/
			
			#loadId {
				display: none;
			}
			
			.loading {
				width: 100%;
				height: 3.25rem;
				text-align: center;
				display: block;
				color: #333;
				font-size: .6rem;
				margin-top: 10px;
				z-index: 1000;
			}
			
			@-webkit-keyframes change {
				0% {
					-webkit-transform: rotate(0deg);
				}
				50% {
					-webkit-transform: rotate(180deg);
				}
				100% {
					-webkit-transform: rotate(360deg);
				}
			}
			
			.loading>img {
				width: .6rem;
				height: .625rem;
				animation: change 3s linear infinite;
			}
			
			.model {
				position: fixed;
				bottom: 10rem;
				left: 50%;
				transform: translate(-50%, 0%);
				width: auto;
				height: 2.2rem;
				padding: 0 0.75rem;
				background: rgba(0, 0, 0, 0.5);
				color: #fff;
				font-size: 0.8rem;
				text-align: center;
				line-height: 2.2rem;
				border-radius: 0.26rem;
				white-space: nowrap;
				display: none;
			}
		</style>
	</head>

